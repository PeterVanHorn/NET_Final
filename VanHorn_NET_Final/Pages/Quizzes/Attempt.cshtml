@page
@model VanHorn_NET_Final.Pages.Quizzes.AttemptModel
@{
}

@foreach(var item in Model.Questions)
{
    <table class="table">
        <tr>
            <p>Question</p>
            <td>@item.QuestionId @item.QuestionText</td>
        </tr>
        <tr>
            @foreach (var option in item.Options)
            {
                <tr>
                <td>< input id="Radio1" type="radio" name="@option.OptionId" class="rb" value="@option.OptionText"/> @option.OptionText </td>
                </tr>

            }
        </tr>
    </table>
}
